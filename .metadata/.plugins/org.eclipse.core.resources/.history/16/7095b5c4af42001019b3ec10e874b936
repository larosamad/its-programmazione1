package _04_arraylist;
import java.util.Date;


public class Product {
	String nome;
	String marca;
	float prezzo;
	Date scadenza;
	 // Costruttore:
	public Product(String nome, String marca, float prezzo, Date scadenza) {
		this.nome = nome;
		this.marca = marca;
		this.prezzo = prezzo;
		this.scadenza = scadenza;
	}
}

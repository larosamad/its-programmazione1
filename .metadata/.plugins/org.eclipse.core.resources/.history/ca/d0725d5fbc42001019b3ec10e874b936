package _05_arraylist;
import java.time.LocalDate;

public class Product {
	private String nome;
	private String marca;
	private double prezzo;
	private LocalDate datascadenza;
	
	
	public Product(String nome, String marca, double prezzo, LocalDate datascadenza) {
		super();
		this.nome = nome;
		this.marca = marca;
		this.prezzo = prezzo;
		this.datascadenza = datascadenza;
	}

	public Product(String nome, String marca, double prezzo) {
		super();
		this.nome = nome;
		this.marca = marca;
		this.prezzo = prezzo;
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}


	public String getMarca() {
		return marca;
	}


	public void setMarca(String marca) {
		this.marca = marca;
	}


	public double getPrezzo() {
		return prezzo;
	}


	public void setPrezzo(double prezzo) {
		this.prezzo = prezzo;
	}

	public LocalDate getDatascadenza() {
		return datascadenza;
	}


	public void setDatascadenza(LocalDate datascadenza) {
		this.datascadenza = datascadenza;
	}

	@Override
	public String toString() {
		return "Product [nome=" + nome + ", marca=" + marca + ", prezzo=" + prezzo + ", datascadenza=" + datascadenza
				+ "]";
	} 
	
}